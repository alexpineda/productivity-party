{
  "name": "party-pipe",
  "version": "0.0.2",
  "private": false,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "login": "bunx --bun @screenpipe/dev@latest login",
    "register": "bunx --bun @screenpipe/dev@latest pipe register --name party-pipe",
    "register:paid": "bunx --bun @screenpipe/dev@latest pipe register --name party-pipe --paid --price 9.99",
    "publish": "bunx --bun @screenpipe/dev@latest pipe publish --name party-pipe",
    "list-versions": "bunx --bun @screenpipe/dev@latest pipe list-versions --name my-pipe",
    "add-components": "bunx --bun @screenpipe/dev@latest components add",
    "logout": "bunx --bun @screenpipe/dev@latest logout",
    "db:schema": "bun scripts/dump-schema.js",
    "db:schema:simple": "bun scripts/simple-schema.js",
    "db:schema:docs": "bun scripts/format-schema.js",
    "partykit:dev": "partykit dev --live",
    "partykit:deploy": "partykit deploy",
    "cron:dev": "node scripts/simulate-cron.js",
    "dev:all": "concurrently \"npm run dev\" \"npm run partykit:dev\" \"npm run cron:dev\"",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@libsql/client": "^0.14.0",
    "@radix-ui/react-accordion": "^1.2.3",
    "@radix-ui/react-alert-dialog": "^1.1.6",
    "@radix-ui/react-avatar": "^1.1.3",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-popover": "^1.1.6",
    "@radix-ui/react-progress": "^1.1.2",
    "@radix-ui/react-select": "^2.1.4",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slider": "^1.1.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.1.3",
    "@radix-ui/react-toast": "^1.2.1",
    "@radix-ui/react-tooltip": "^1.1.6",
    "@screenpipe/browser": "^0.1.36",
    "@screenpipe/js": "^1.0.17",
    "@supabase/supabase-js": "^2.49.1",
    "@types/lodash": "^4.17.16",
    "@types/nodemailer": "^6.4.17",
    "ai": "^4.0.18",
    "cheerio": "^1.0.0",
    "class-variance-authority": "^0.7.1",
    "cmdk": "^1.0.4",
    "dotenv": "^16.4.7",
    "framer-motion": "^12.4.7",
    "js-levenshtein": "^1.1.6",
    "lodash": "^4.17.21",
    "lucide-react": "^0.477.0",
    "next": "^15.1.7",
    "nodemailer": "^6.9.16",
    "openai": "^4.76.3",
    "partysocket": "^1.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^9.0.1",
    "react-syntax-highlighter": "^15.5.0",
    "remark-gfm": "^4.0.0",
    "remark-math": "^6.0.0",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.13",
    "@types/js-levenshtein": "^1.1.3",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/react-syntax-highlighter": "^15.5.13",
    "bun-types": "latest",
    "concurrently": "^8.2.2",
    "eslint": "^8",
    "eslint-config-next": "14.2.4",
    "partykit": "0.0.111",
    "postcss": "^8",
    "raw-loader": "^4.0.2",
    "tailwindcss": "^3.4.1",
    "typescript": "^5",
    "vitest": "^3.0.7"
  }
}
